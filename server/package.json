{
  "name": "landai-assistant-server",
  "type": "module",
  "imports": {
    "#*": "./*.js"
  },
  "node-pg-migrate": {
    "databaseUrl": "postgresql://postgres:1221qaApo@localhost:5432/landai_db",
    "dir": "db/migrations",
    "migrationsTable": "pgmigrations"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch --env-file=.env server.js",
    "test": "vitest",
    "db:schema": "psql -d landai_db -f db/schema.sql",
    "db:seed": "node --env-file=.env db/seed.js",
    "db:reset": "npm run db:schema && npm run db:seed",
    "db:render": "node db/seed.js",
    "migrate": "node --env-file=.env node_modules/node-pg-migrate/bin/node-pg-migrate",
    "migrate:create": "npm run migrate create --"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.0",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^17.1.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "https-proxy-agent": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^4.1.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.3.31",
    "pg": "^8.14.1",
    "puppeteer": "^24.12.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "node-pg-migrate": "^8.0.3",
    "supertest": "^7.1.0",
    "vitest": "^3.1.2"
  }
}
